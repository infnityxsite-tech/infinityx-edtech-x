{
  "version": 2,
  "buildCommand": "pnpm install && pnpm build",
  "installCommand": "pnpm install",
  "framework": null,
  "outputDirectory": "dist/client",
  "builds": [
    {
      "src": "server/_core/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/_core/index.ts"
    },
    {
      "source": "/uploads/:match*",
      "destination": "/uploads/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/server/_core/index.ts"
    }
  ]
}
